public Boolean duplicateContacts(Contact c1, Contact c2) {
    // Check if email addresses are equal (but only if both are not null)
    if (c1.Email != null && c2.Email != null && c1.Email == c2.Email) {
        return true;
    }

    // Check if phone numbers are equal (but only if both are not null)
    if (c1.Phone != null && c2.Phone != null && c1.Phone == c2.Phone) {
        return true;
    }

    // If no matches found, return false
    return false;
}